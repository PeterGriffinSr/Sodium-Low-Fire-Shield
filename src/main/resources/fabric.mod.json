{
  "schemaVersion": 1,
  "id": "low_fire_shield",
  "version": "${version}",
  "name": "Low Fire/Shield",
  "description": "integration that adds low fire and low shield",
  "authors": [
    "Codezz-ops"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/low-fire-shield",
    "sources": "https://github.com/PeterGriffinSr/low-fire-shield",
    "issues": "https://github.com/PeterGriffinSr/low-fire-shield/issues"
  },
  "license": "LGPLv3",
  "icon": "assets/low_fire_shield/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "com.codezz.low_fire_shield.client.LowFireShieldDataGenerator"
    ],
    "client": [
      "com.codezz.low_fire_shield.client.LowFireShieldClient"
    ],
    "main": [
      "com.codezz.low_fire_shield.Clearview"
    ]
  },
  "mixins": [
    "low_fire_shield.mixins.json",
    {
      "config": "low_fire_shield.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
